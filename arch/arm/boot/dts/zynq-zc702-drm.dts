		i2c0: i2c@0 {
			compatible = "i2c-gpio";
			gpios = <&gpio 51 0 &gpio 50 0>;
			i2c-gpio,delay-us = <2>;
			#address-cells = <1>;
			#size-cells = <0>;

			mux@74 {
				compatible = "pca9548";
				reg = <0x74>;
